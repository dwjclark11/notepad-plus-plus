#!/bin/sh
# prerm script for notepad-plus-plus

set -e

case "$1" in
    remove|upgrade|deconfigure)
        # Nothing to do before removal
    ;;

    failed-upgrade)
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

